    for j=1:length(nx_int)
       if agrup(i)==agrup(j)
           dc(nx_int(i),ny_int(i))=rand(1,1);
       end
    end
    
    
    
    
    
    
   if i==1
       dc(nx_int(i),ny_int(i))=rand(1,1);
   else 
       agrup(i)==agrup
       
   dc(nx_int(i),ny_int(i))=rand(1,1); 
